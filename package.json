{
  "name": "isard",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "dev": "ng serve --configuration=dev",
    "start prod": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint:html": "eslint \"src/app/**/*.html\" --max-warnings=0",
    "lint:scss": "stylelint \"src/**/*.scss\" --max-warnings=0",
    "lint:script": "eslint \"src/**/*.{js,ts}\" --max-warnings=0",
    "lint:all": "yarn run lint:html && yarn run lint:script && yarn run lint:scss",
    "lint:scss:fix": "yarn run lint:scss --fix",
    "lint:script:fix": "yarn run lint:script --fix",
    "lint:html:fix": "yarn run lint:html --fix",
    "lint:all:fix": "yarn run lint:html:fix && yarn run lint:script:fix && yarn run lint:scss:fix"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.0.1",
    "@angular/compiler": "^20.0.1",
    "@angular/core": "^20.0.1",
    "@angular/forms": "^20.0.1",
    "@angular/platform-browser": "^20.0.1",
    "@angular/platform-browser-dynamic": "^20.0.1",
    "@angular/router": "^20.0.1",
    "@colsen1991/ngx-translate-extract-marker": "^3.0.1",
    "@eslint/js": "^9.28.0",
    "@ngx-translate/core": "^16.0.4",
    "@ngx-translate/http-loader": "^16.0.1",
    "bootstrap": "^5.3.6",
    "flag-icons": "^7.5.0",
    "rxjs": "~7.8.2",
    "tslib": "^2.3.0",
    "uuid": "^11.1.0",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.0.1",
    "@angular/cli": "^20.0.1",
    "@angular/compiler-cli": "^20.0.2",
    "@types/jasmine": "~5.1.8",
    "angular-eslint": "20.0.0",
    "eslint": "^9.28.0",
    "stylelint": "^16.20.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "stylelint-prettier": "^5.0.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "8.33.1"
  }
}
